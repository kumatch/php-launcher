{
    "name": "kumatch/launcher",
    "description":"A service launcher using a Symfony DependencyInjection Component.",
    "type":"library",
    "homepage": "https://github.com/kumatch/php-launcher",
    "keywords":["service", "launcher", "container", "di", "dependency injection"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Yosuke Kumakura",
            "homepage": "https://github.com/kumatch"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/dependency-injection": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "3.*",
        "symfony/config": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "Kumatch\\": "src/"
        }
    }
}
